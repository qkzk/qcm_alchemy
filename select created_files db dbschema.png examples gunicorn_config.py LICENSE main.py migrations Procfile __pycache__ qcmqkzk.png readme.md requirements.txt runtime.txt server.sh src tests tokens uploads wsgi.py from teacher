    TABLE "qcm" CONSTRAINT "qcm_id_teacher_fkey" FOREIGN KEY (id_teacher) REFERENCES teacher(id) ON DELETE CASCADE
    TABLE "reset_key" CONSTRAINT "reset_key_id_teacher_fkey" FOREIGN KEY (id_teacher) REFERENCES teacher(id) ON DELETE CASCADE
